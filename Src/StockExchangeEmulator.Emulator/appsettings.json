{
  "Serilog": {
        "Using": [
            "Serilog.Sinks.Console",
            "Serilog.Sinks.File"
        ],
        "MinimumLevel": {
            "Default": "Information"
        },
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "File",
                "Args": {
                    "path": "../logs/app-.log",
                    "rollingInterval": "Day",
                    "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {Level:u3}] {Message:lj}{NewLine}{Exception}",
                    "retainedFileCountLimit": 3
                }
            }
        ],
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithThreadId"
        ]
    },
  "Settings": {
    "InitialPrice": 5,
    "ChangeBehaviorInterval": 5,
    "ChangePriceInterval": 1,
    "StrategyType": "Simple",
    "DefaultCount": 200,
    "DefaultBuy": 5,
    "DefaultSell": 5,
    "DefaultSum": 1000
  },
  "JobConfigs": {
    "ChangePriceBehaviourJobConfig" :
    {
      "IntervalInSeconds" : 60
    },
    "ChangePriceJobConfig" :
    {
      "IntervalInSeconds" : 10
    }
  }
}
